{
  "name": "todo-server",
  "version": "0.0.0",
  "private": false,
  "scripts": {
    "start": "npm run build && node dist/index.js",
    "dev": "nodemon src/index.js --exec \"nodemon -r dotenv/config -r babel-register\"",
    "remove": "rimraf dist",
    "build": "npm run remove && mkdir -p dist && babel src -p -s -D -d dist"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.15.5",
    "mongodb": "^2.2.30",
    "mongoose": "^4.8.7",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-eslint": "^8.0.3",
    "babel-register": "^6.26.0",
    "dotenv": "^4.0.0",
    "eslint": "^4.12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "rimraf": "^2.6.2"
  }
}

